def comp2(array1, array2):
    '''
    2 lists provided. First list (array1) contains integer, second list(array2) should contain the square of
    each element in List 1
    '''

    a1_lst = sorted(array1)
    a2_lst = sorted(array2)
    
    print(a1_lst)
    print(a2_lst)
    
    return a1_lst == [] and a2_lst == []

    return a1_lst == None and a2_lst == []
    
    for x in a1_lst:
        #print(array1)
        if x*x in a2_lst:
            return True
        else:
            return False
